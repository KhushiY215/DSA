//ctrl+alt+n
#include <iostream>
#include <string>
using namespace std;

string brackets(string s) {
    string s2;
    for (int i = 0; i < s.size(); i++) {
        if (s[i] == '.') {
            s2 += "[.]";
        } else {
            s2 += s[i];
        }
    }
    return s2;
}

int main() {
    string s;
    cin >> s;
    cout << brackets(s);
    return 0;
}
