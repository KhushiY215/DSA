
#include<bits/stdc++.h>
#include <iostream>
using namespace std;

int main() {
    int len=0;
    string s="abcdefabcd";
    vector<int>lps(s.size(),0);
    int prefix=0,suffix=1;
    
    while(suffix<s.size()){
        if(s[prefix]==s[suffix]){
            lps[suffix]=prefix+1;
            prefix++;
            suffix++;
        }
        else{
            if(prefix==0){
                lps[suffix]=0;
                suffix++;
            }
            else{
                prefix=lps[prefix-1];
            }
        }
    }
    cout<<lps[s.size()-1];
    return 0;
}
