//length of loop in LL

int lengthOfLoop(Node* head) {
    Node* slow = head;
    Node* fast = head;
    while (fast != nullptr && fast->next != nullptr) {
        slow = slow->next;     
        fast = fast->next->next;
        if (slow == fast) {
            return findLength(slow, fast);
        }
    }
    return 0; 
}
