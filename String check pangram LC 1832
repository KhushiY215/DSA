class Solution {
public:
    bool checkIfPangram(string sentence) {
        vector<bool> ar(26,0);
        for(int i=0;i<sentence.size();i++){
            ar[sentence[i]-'a']=1;
        }
        for(int i=0;i<26;i++){
            if(ar[i]==0)
            return false;
        }
        return true;
    }
};
